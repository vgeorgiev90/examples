APP_LOGO={{ app_logo }}
APP_NAME={{ app_name }}
APP_ENV=local
APP_KEY={{ app_key }}
APP_DEBUG={{ debug }}
APP_URL={{ app_url }}

LOG_CHANNEL=single

DB_CONNECTION=mysql
DB_HOST={{ db_host }}
DB_PORT=3306
DB_DATABASE={{ db_name }}
DB_USERNAME={{ db_user }}
